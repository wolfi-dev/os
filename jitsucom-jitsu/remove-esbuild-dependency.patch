 packages already

---
 pnpm-workspace.yaml            | 1 -
 webapps/console/next.config.js | 1 -
 2 files changed, 2 deletions(-)

diff --git a/pnpm-workspace.yaml b/pnpm-workspace.yaml
index 8ab7e8f6b..175a7c873 100644
--- a/pnpm-workspace.yaml
+++ b/pnpm-workspace.yaml
@@ -13,7 +13,6 @@ onlyBuiltDependencies:
   - core-js
   - core-js-pure
   - cpu-features
-  - esbuild
   - isolated-vm
   - prisma
   - protobufjs
diff --git a/webapps/console/next.config.js b/webapps/console/next.config.js
index 6854722dd..cdfc6e59d 100644
--- a/webapps/console/next.config.js
+++ b/webapps/console/next.config.js
@@ -57,7 +57,6 @@ module.exports = withBundleAnalyzer({
     "*": [
       "./**/node_modules/@swc/core-linux-x64-gnu",
       "./**/node_modules/@swc/core-linux-x64-musl",
-      "./**/node_modules/esbuild/linux",
       "./**/node_modules/webpack",
       "./**/node_modules/rollup",
       "./**/node_modules/terser",
