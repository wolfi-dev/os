Even though we build for a specific Python version, poetry will error out if it
can't resolve dependencies for all supported Python versions.  This bumps the
minimum version to the version we currently use, to reduce the set of versions
that libraries need to match (specifically to upgrade cryptography in this
instance).
---
diff --git a/python/kserve/pyproject.toml b/python/kserve/pyproject.toml
index 63befa0..3e3327a 100644
--- a/python/kserve/pyproject.toml
+++ b/python/kserve/pyproject.toml
@@ -5,7 +5,7 @@ authors = [
     {name = "The KServe Authors", email = "hejinchi@cn.ibm.com"},
 ]
 license = {text = "Apache-2.0"}
-requires-python = "<3.13,>=3.9"
+requires-python = "<3.13,>=3.11"
 dependencies = [
     "uvicorn[standard]<1.0.0,>=0.30.6",
     "fastapi>=0.115.3",
diff --git a/python/storage/pyproject.toml b/python/storage/pyproject.toml
index f48d99a07..2e45daef6 100644
--- a/python/storage/pyproject.toml
+++ b/python/storage/pyproject.toml
@@ -4,7 +4,7 @@ authors = [
     {name = "The KServe Authors", email = "fspolti@redhat.com"},
 ]
 license = {text = "https://github.com/kserve/kserve/blob/master/LICENSE"}
-requires-python = "<3.13,>=3.9"
+requires-python = "<3.13,>=3.11"
 dependencies = [
     "requests<3.0.0,>=2.32.2",
     "google-cloud-storage<3.0.0,>=2.14.0",
